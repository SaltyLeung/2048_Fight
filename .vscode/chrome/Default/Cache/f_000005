        if (1 !== dt && 0 !== dt) {
          dt *= 2;
          dt = dt < 1 ? .5 * Math.pow(2, 10 * (dt - 1)) : .5 * (2 - Math.pow(2, -10 * (dt - 1)));
        }
        this._inner.update(dt);
      },
      reverse: function() {
        return new cc.EaseExponentialInOut(this._inner.reverse());
