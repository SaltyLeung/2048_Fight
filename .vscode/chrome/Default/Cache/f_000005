    cc.EaseElastic = cc.ActionEase.extend({
      _period: .3,
      ctor: function(action, period) {
        cc.ActionEase.prototype.ctor.call(this);
        action && this.initWithAction(action, period);
      },
      getPeriod: function() {
        return this._period;
      },
      setPeriod: function(period) {
        this._period = period;
      },
