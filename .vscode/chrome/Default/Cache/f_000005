        if (rotation) {
          c = Math.sin(rotation);
          d = Math.cos(rotation);
          a = d;
          b = -c;
        }
        var zoomRatio = this.zoomRatio;
        a *= zoomRatio;
        b *= zoomRatio;
        c *= zoomRatio;
